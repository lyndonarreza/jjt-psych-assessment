
Set-Content -Path .gitignore -Encoding UTF8 -Value @"
# Python / Django
__pycache__/
*.py[cod]
*.egg-info/
db.sqlite3
/media/
staticfiles/
static_collected/

# Virtual env & secrets
env/
venv/
.env
*.env

# OS / IDE
.DS_Store
Thumbs.db
.vscode/
.idea/

# Migrations pyc caches
**/migrations/__pycache__/

# stray local files
/100
/50
"@
